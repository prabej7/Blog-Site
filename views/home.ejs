<%- include("partials/header") -%>

    <h1>Home</h1>
    <p>
        <%= homeText %>
    </p>
    <%= mainPostH %>

        <% mainPostH.forEach((element)=>{ %>
            <h2>
                <%= element.postTitle %>
            </h2>
            <div class="align">
                <p>
                    <%= element.postBody.slice(0,100)+"..." %>
                </p>
                <a href="posts/<%= element.postTitle %>">Read more</a>
            </div>
            <% }); %>
                <%- include("partials/footer") -%>